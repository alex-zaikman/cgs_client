//					checkAnswerOnState:true,
//					isAssessment:true,
//					allExposed:true,
//					readOnly:true
//				}
//			},
//
//			'ANSWER_VIEW': {
//				teacher: {
//					lastTaskFocusing:true,
//					showCorrectAnswerButton:true,
//					progressClass:t2k.player.task.progress.AssessableProgress,
//					checkAnswerOnState:true,
//					isAssessment:true,
//					readOnly:true,
//					allExposed:true
//				},
