(function () {

    t2k.util.TabletUtil = Object.subClass({

        bindButtonTouchStartAndEnd:function ($elem) {
            $elem.bind('touchstart', function () {
                $elem.addClass('selected');
            });

            $elem.bind('touchend', function () {
                $elem.removeClass('selected');
            });
        }


    });

})();

