        layout : 'inline',
        /** The mustache template to render. */
        template : TEMPLATE
    }; // End of defaultConfig.

    t2k.component.group.GroupView = t2k.core.View.subClass({

        /**
         * @constructor
         * @see superclass documentation
         */
        ctor : function(config) {
            // Delegate.
            this._super(copy(config, defaultConfig));
        }

    });

})();

