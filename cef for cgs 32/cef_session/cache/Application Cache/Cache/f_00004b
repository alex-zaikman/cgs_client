                    minusSign:false
                },

                structures:{
                    power:false,
                    remainder:false
                },

                logic:{
                    last:false
                }
            },

            relations:{
                symbols:{
                    operators:false,
                    relations:false,
                    closingParenthesis:false,
                    closingSquareParenthesis:false,
                    decimalPoint:false,
                    thousandsComma:false,
                    percent:false
                },

                structures:{
                    power:false,
                    remainder:false
                },

                logic:{
                    first:false,
                    last:false
                }
            },

            openingParenthesis:{
                symbols:{
                    operators:false,
                    relations:false,
                    closingParenthesis:false,
                    openingSquareParenthesis:false,
                    closingSquareParenthesis:false,
