// Cloze constant values
t2k.component.cloze.ClozeConstants = {	
	clozeSpacing : 30,
	childWidthReduction: 0,
    'errorClass' : 'masc_error',
    'errorText' : 'ERROR : width of the cloze task is not wide enough to contain the bank component'

};


