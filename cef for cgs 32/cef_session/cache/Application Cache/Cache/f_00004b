(function() {
    /**
     * @class t2k.component.answer.ClozeAnswer
     * @desc A cloze task class
     * @namespace t2k.component.answer
     * @extends t2k.component.answer.BaseClozeAnswer
     */
// ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
