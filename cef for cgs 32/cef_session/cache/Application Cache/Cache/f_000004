/**
 *  alexander.tokar 23/07/12 16:09
    ________________________________________________________________________________________
       The CSS class' name      |	Type                |   Additions
    ____________________________|_______________________|___________________________________
        question	            |    main , wrapper     |
        answer	                |    main , wrapper     |   main{
        instruction	            |    main , wrapper     |       margin-bottom:-1.5rem;
        layoutGroup	            |    layout , wrapper   |       margin-right(left):-1.5rem;
        questionGroup	        |    layout , wrapper   |   }
        subQuestion	            |    layout , wrapper   |
        clozeArea	            |    layout , wrapper   |   layout { margin:0; }
        mtqArea	                |    layout , wrapper   |
        mtqMultiSubQuestion	    |    layout , wrapper   |   wrapper > first-children's {
        mtqSubQuestion	        |    layout             |       margin-right(left):1.5rem;
        options	                |    layout , wrapper   |       margin-bottom:1.5rem;
        multiSubAnswer	        |    layout , wrapper   |   }
        bank                    |    wrapper            |
        bank_readonly           |    wrapper            |   single > first-children's {
        definition	            |    single             |       margin:0;
        cell	                |    single             |   }
        subAnswer	            |    single             |
        option	                |    single             |
    ____________________________|_______________________|___________________________________
 */

