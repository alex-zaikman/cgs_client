			},
			
			"READ_ONLY": {
				student: {
					readOnly:true,
					progressClass:t2k.player.task.progress.ReadOnlyProgress,
					activateTaskOnClick: false,
					allExposed:true,
					lastTaskFocusing: true
				},
			},
			
			"WORK_OVERVIEW": {
				teacher: {
					allExposed:true,
					forceUserFinalAttempt: true,
					readOnly:true,
					showCorrectAnswerButton:true
				}
			},
			
