            this._super(flag);

            // set progress = ready on flag = true
            if (flag) {
                this.cfg.eventsManager.dispatchEvent('setProgressReady');
            }
        },

        getState:function () {
            // create xml
            var state = this._super();
            // collect all text editor state from view

            return state;
        },

        setState:function (state) {
            this._super(state);
            // set state for all state items
        }

    });

// ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
