/**
 * Class t2k.util.services
 */
(function() {

    t2k.util.services = Object.subClass({
        
        /**
         * ctor
         */
        ctor : function () {
            this.reset();
        },

        /**
         * add
         */
        add : function(itemName, item){
           this._item[itemName] = item;
        },

        /**
         * get
         */
        get : function(itemName){
            return this._item[itemName] || function(){};
        },

        reset : function(){
            this._item = [];
        }

    });

})();

