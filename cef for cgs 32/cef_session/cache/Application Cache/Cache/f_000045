        /**
         * @constructor
         * @see superclass documentation
         */
        ctor: function(config) {
            config = override(config, defaultConfig);
            this._super(config);
        },
        /**
         * initScroll
         * listen to the scroll event of scrollable div, in order to slide bank element up/down
         */
        initScroll : function(flag) {
            
        }
    })
        
})();
