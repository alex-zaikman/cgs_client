        ctor: function(config) {
            // Delegate
            this._super(config);

            this.view = this.createNewView(t2k.component.definition.DefinitionView, this.cfg);
            this.startComposite2(copy({parent: this.view.cfg.id + '_content'}, config));
        }

    });

// ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
